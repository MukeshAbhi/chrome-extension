{
  "manifest_version": 3,
  "name": "Read Time Extension",
  "version": "1.0",
  "description": "A Chrome extension that calculates reading time.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage", "activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["src/contentScript.js"]
    }
  ]
}
